<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.GridPane?>
<GridPane xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="4" vgap="4" prefHeight="128.0" prefWidth="250.0">
	<Label text="Monnaie 1 :" GridPane.columnIndex="0" GridPane.rowIndex="0" />
	<TextField fx:id="field1" GridPane.columnIndex="1" GridPane.rowIndex="0" />
	<Label text="Monnaie 2 :" GridPane.columnIndex="0" GridPane.rowIndex="1" />
	<TextField fx:id="field2" GridPane.columnIndex="1" GridPane.rowIndex="1" />
	<Label text="Parité 1 à 2 :" GridPane.columnIndex="0" GridPane.rowIndex="2" />
	<TextField fx:id="field3" GridPane.columnIndex="1" GridPane.rowIndex="2" />
	<Button text="Sauvegarder" onAction="#save" GridPane.columnIndex="0" GridPane.rowIndex="3" />
</GridPane>