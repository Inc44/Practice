<GridPane xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="16" vgap="16" prefHeight="36.0" prefWidth="140.0">
	<Button fx:id="btnDecrement" text="-" GridPane.columnIndex="0" GridPane.rowIndex="0" />
	<Label fx:id="label1" GridPane.columnIndex="1" GridPane.rowIndex="0" />
	<HBox spacing="0" GridPane.columnIndex="2" GridPane.rowIndex="0">
		<Button fx:id="btnIncrement" text="+" />
		<Button fx:id="btnReset" text="reset" />
	</HBox>
</GridPane>